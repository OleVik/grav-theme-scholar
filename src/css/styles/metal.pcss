@import "../styles.base.pcss";

:root {
  --primary: #bd8c7d;
  --secondary: #d1bfa7;
  --text: #3c3b3b;
  --text-alt: #ffffff;
  --background: #f4f4f4;
  --foreground: #ffffff;
  --border-color: rgba(160, 160, 160, 0.3);
  --header-background: var(--primary);
  --aside-text: #e8e8e6;
  --aside-text-active: #ffffff;
  --aside-background: #8e8e90;
  --aside-background-active: #7a7a7c;
  --aside-background-active-state: #3e3e40;
}
