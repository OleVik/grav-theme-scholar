@import "../styles.base.pcss";

:root {
  --primary: #e36171;
  --secondary: #f08e5c;
  --text: #191919;
  --text-alt: #e6e6e6;
  --background: #fbfd98;
  --foreground: var(--secondary);
  --border-color: #f3b873;
  --header-background: var(--primary);
  --aside-text: #e8e8e6;
  --aside-text-active: #ffffff;
  --aside-background: var(--secondary);
  --aside-background-active: #ee773e;
  --aside-background-active-state: #c06b56;
}
