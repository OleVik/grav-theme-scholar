@import "../styles.base.pcss";

:root {
  --primary: #2e5797;
  --secondary: #ef8846;
  --text: #ffffff;
  --text-alt: #000000;
  --background: #d0c469;
  --foreground: var(--secondary);
  --border-color: #ca8d41;
  --header-background: var(--primary);
  --aside-text: #e8e8e6;
  --aside-text-active: #ffffff;
  --aside-background: var(--secondary);
  --aside-background-active: #ee7f3a;
  --aside-background-active-state: #dc635e;
}
