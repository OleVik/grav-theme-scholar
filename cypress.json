      "article": "/article",
      "blog": "/blog",
      "cv": "/cv",
      "docs": "/docs"
    },
    "elements": {
      "index": ["header[role=\"banner\"] .menu .search-button", "a"],
      "article": ["a"],
      "blog": ["a"],
      "cv": ["a"],
      "docs": ["a"]
    }
  }
}
