{% extends 'default.html.twig' %}

{% set body_classes = (body_classes|default('blog page listing')) %}

{% block article %}

{% with {
    'page': page,
    'collection': page.collection
    } %}
    {{ block('content', 'partials/blog/listing.html.twig') }}
{% endwith %}
<aside>
    <header>
        <h1>{{ page.title }}</h1>
        {{ page.content }}
    </header>
    {% with {
        'page': page,
        'collection': page.collection('aside')
        } %}
        {{ block('content', 'partials/blog/listing.html.twig') }}
    {% endwith %}
</aside>

{% endblock %}